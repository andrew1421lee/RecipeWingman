{
    "manifest_version": 2,
    "name": "Recipe to Wegmans",
    "description": "Get cost of Recipe ingredients",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Re2Weg",
        "default_popup": "appmenu.html"
    },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "src/content.js",
                "src/jquery-3.1.1.js"
            ],
            "css":[
                "css/popup.css"
            ]
        }
    ],
    "background": {
        "scripts": ["src/bckgrd.js"]
    },
    "permissions":[
        "activeTab", "storage", "http://127.0.0.1:5000/*", "contextMenus", "background"
    ]
}